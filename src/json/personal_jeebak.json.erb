                    "type": "basic",
                    "from": <%= from("1", [], ["caps_lock"]) %>,
                    "to": <%= to([["f1", []]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("2", [], ["caps_lock"]) %>,
                    "to": <%= to([["f2", []]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("3", [], ["caps_lock"]) %>,
                    "to": <%= to([["f3", []]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("4", [], ["caps_lock"]) %>,
                    "to": <%= to([["f4", []]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("5", [], ["caps_lock"]) %>,
                    "to": <%= to([["f5", []]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("6", [], ["caps_lock"]) %>,
                    "to": <%= to([["f6", []]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("7", [], ["caps_lock"]) %>,
                    "to": <%= to([["f7", []]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("8", [], ["caps_lock"]) %>,
                    "to": <%= to([["f8", []]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("9", [], ["caps_lock"]) %>,
                    "to": <%= to([["f9", []]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("0", [], ["caps_lock"]) %>,
                    "to": <%= to([["f10", []]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("hyphen", [], ["caps_lock"]) %>,
                    "to": <%= to([["f11", []]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("equal_sign", [], ["caps_lock"]) %>,
                    "to": <%= to([["f12", []]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },

                <%# -------------------------------------------------- %>
                <%# Misc Personalizations {;,',/} %>
                <%# Space-; to ⌘-⌃-; [custom shortcut to: Move left a space] %>
                <%# Space-' to ⌘-⌃-' [custom shortcut to: Move right a space] %>
                <%# Space-/ to ⌘--/ [custom shortcut to: Toggle iTerm maximized] %>
                <%# Space-right_shift to ⌘-⌃--/ [custom shortcut to: Application windows] %>
                <%# -------------------------------------------------- %>
                {
                    "type": "basic",
                    "from": <%= from("semicolon", [], ["caps_lock"]) %>,
                    "to": <%= to([["semicolon", ["left_command", "left_control"]]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("quote", [], ["caps_lock"]) %>,
                    "to": <%= to([["quote", ["left_command", "left_control"]]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("slash", [], ["caps_lock"]) %>,
                    "to": [
                        { "shell_command": "~/.zgen/jeebak/devtools-master/bin/maccomplice togg_app iTerm2" }
                    ],
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("right_shift", [], ["caps_lock"]) %>,
                    "to": <%= to([["slash", ["left_command", "left_control"]]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },

                <%# -------------------------------------------------- %>
                <%# Space-↩ to mission_control %>
                <%# -------------------------------------------------- %>
                {
                    "type": "basic",
                    "from": <%= from("return_or_enter", [], ["caps_lock"]) %>,
                    "to": <%= to([["mission_control"]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },

                <%# -------------------------------------------------- %>
                <%# Media keys %>
                <%# Space-⎋, and Space-` to rewind %>
                <%# Space-⌫  to fastforward %>
                <%# Space-y to mute %>
                <%# Space-[ to volume_decrement %>
                <%# Space-] to volume_increment %>
                <%# Space-\ to play_or_pause %>
                <%# -------------------------------------------------- %>
                {
                    "type": "basic",
                    "from": <%= from("escape", [], ["caps_lock"]) %>,
                    "to": <%= to([["rewind"]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("grave_accent_and_tilde", [], ["caps_lock"]) %>,
                    "to": <%= to([["rewind"]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("delete_or_backspace", [], ["caps_lock"]) %>,
                    "to": <%= to([["fastforward"]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("y", [], ["caps_lock"]) %>,
                    "to": <%= to([["mute"]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("open_bracket", [], ["caps_lock"]) %>,
                    "to": <%= to([["volume_decrement"]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("close_bracket", [], ["caps_lock"]) %>,
                    "to": <%= to([["volume_increment"]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("backslash", [], ["caps_lock"]) %>,
                    "to": <%= to([["play_or_pause"]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },

                <%# -------------------------------------------------- %>
                <%# normalize punctuation marks %>
                <%# -------------------------------------------------- %>
                {
                    "type": "basic",
                    "from": <%= from("non_us_pound", [], ["caps_lock"]) %>,
                    "to": <%= to([["spacebar"], ["non_us_pound"], ["vk_none"]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("non_us_backslash", [], ["caps_lock"]) %>,
                    "to": <%= to([["spacebar"], ["non_us_backslash"], ["vk_none"]]) %>,
                    "conditions": [
                        { "type": "variable_if", "name": "touchcursor_extended_mode", "value": 1 }
                    ]
                }

                <%# -------------------------------------------------- %>
                <%# normalize remaining unbound letter keys %>
                <%# -------------------------------------------------- %>
            ]
        },
        {
            "description": "Tab Modifier Mode [Tab as Trigger Key]",
            "manipulators": [

                <%# -------------------------------------------------- %>
                <%# press ⇥ to enter Tab Modifier Mode, release to quit %>
