{
    "title": "Personal rules (@wataash)",
    "rules": [

        {
            "description": "left_control+hyphen, left_control+equal to ----------,========== (inspired by @tekezo)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("hyphen", ["left_control"], ["caps_lock"]) %>,
                    "to": [
                        { "key_code": "hyphen" }, { "key_code": "hyphen" },
                        { "key_code": "hyphen" }, { "key_code": "hyphen" },
                        { "key_code": "hyphen" }, { "key_code": "hyphen" },
                        { "key_code": "hyphen" }, { "key_code": "hyphen" },
                        { "key_code": "hyphen" },
                        {
                            "key_code": "hyphen",
                            "repeat": false
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("equal_sign", ["left_control"], ["caps_lock"]) %>,
                    "to": <%= to([
                        ["equal_sign"], ["equal_sign"], ["equal_sign"], ["equal_sign"], ["equal_sign"],
                        ["equal_sign"], ["equal_sign"], ["equal_sign"], ["equal_sign"], ["equal_sign"],
                        ["vk_none"],
                    ]) %>
                }
            ]
        },

